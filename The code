#include <Servo.h>
void setup()
{
  
  Servo motor1;
  Servo motor2;
  Servo motor3;
  Servo motor4;
  Servo motor5;
  
  
  motor1.attach(10);
  motor2.attach(9);
  motor3.attach(8);
  motor4.attach(3);
  motor5.attach(2);
  
  
  motor1.write(90);
  motor2.write(90);
  motor3.write(90);
  motor4.write(90);
  motor5.write(90);
  
 // delay(1000);
  
 
}

void loop()
{
 
}
