#include <Servo.h>

void setup()
{
  
  Servo motor1;
  motor1.attach(13);
  motor1.write(90);
  
  Servo motor2;
  motor2.attach(12);
  motor2.write(90);
  
  Servo motor3;
  motor3.attach(11);
  motor3.write(90);
  
  Servo motor4;
  motor4.attach(3);
  motor4.write(90);
  
  Servo motor5;
  motor5.attach(2);
  motor5.write(90);
  
   delay(1000);
  
}

void loop()
{
 
}
